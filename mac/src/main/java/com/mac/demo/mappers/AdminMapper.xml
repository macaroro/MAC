<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mac.demo.mappers.AdminMapper">


<select id="findAllUser" resultType="com.mac.demo.model.User">
		SELECT * FROM user_tb
	</select>
	<select id="findAllFreeBoard" resultType="com.mac.demo.model.Board">
		SELECT * FROM free_board
	</select>
	<select id="findAllAdsBoard" resultType="com.mac.demo.model.Board">
		SELECT * FROM ads_board
	</select>
	
	<delete id="freeBordDeleted" parameterType="String">
		DELETE FROM free_board WHERE idMac=#{idMac}
	</delete>
	
	<delete id="adsBordDeleted" parameterType="String">
		DELETE FROM ads_board WHERE idMac=#{idMac}
	</delete>
	
	<delete id="UserDeleted" parameterType="String">
		DELETE FROM user_tb WHERE idMac=#{idMac}
	</delete>



</mapper>